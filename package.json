{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run nextDev\" \"npm run wundergraph\" \"npm run browser\"",
    "wundergraph": "wunderctl up --debug",
    "browser": "wait-on \"http-get://localhost:3000\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3000",
    "build": "next build",
    "nextDev": "next dev",
    "nextStart": "next start",
    "generate": "wunderctl generate --debug",
    "wunderctl": "wunderctl",
    "migrate": "cd database && prisma format && prisma migrate dev --name"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@wundergraph/nextjs": "^0.1.6",
    "@wundergraph/sdk": "^0.100.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.3.0",
    "konva": "^8.3.12",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.0",
    "next": "^12.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-konva": "^18.2.1",
    "react-konva-utils": "^0.3.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "@types/react": "^18.0.7",
    "autoprefixer": "^10.4.8",
    "concurrently": "^6.0.0",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.16",
    "prisma": "^4.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.0"
  }
}
